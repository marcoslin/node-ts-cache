{
  "name": "@marcoslin/node-ts-cache",
  "version": "3.2.2",
  "description": "Simple and extensible caching module supporting decorators",
  "main": "lib/index",
  "scripts": {
    "test": "TS_NODE_PROJECT=test/tsconfig.json mocha -r ts-node/register -r tsconfig-paths/register test/**/*.test.ts",
    "tdd": "mocha -w",
    "tdd-debug-brk": "mocha --inspect-brk",
    "build": "tsc --project src/tsconfig.json --declaration",
    "prepublish": "npm run build",
    "clean": "git clean -fdx src",
    "prepare": "npm run build",
    "dev": "tsc -p . -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcoslin/node-ts-cache.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "cache",
    "typescript",
    "ts",
    "caching",
    "memcache",
    "memory-cache",
    "redis-cache",
    "redis",
    "cache",
    "file-cache",
    "node-cache",
    "ts-cache"
  ],
  "author": "Himmet Avsar <avsar.himmet1@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcoslin/node-ts-cache/issues"
  },
  "homepage": "https://github.com/marcoslin/node-ts-cache#readme",
  "publishConfig": { "registry": "https://npm.pkg.github.com/" },
  "dependencies": {
    "@types/bluebird": "3.5.27",
    "@types/node": "10.14.17",
    "@types/redis": "^2.8.13",
    "bluebird": "3.5.5",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "5.0.5",
    "mocha": "5.2.0",
    "proxyquire": "2.1.3",
    "sinon": "7.4.2",
    "ts-node": "8.3.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.6.2"
  },
  "optionalDependencies": {}
}
